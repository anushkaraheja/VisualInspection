{
  "name": "BrickRed Systems Vision AI Portal",
  "description": "The Vision AI Portal by BrickRed Systems delivers accessible AI-powered solutions across every industry. Empower your business with cutting-edge AI, Cloud, and Data solutions tailored to your needs.",
  "repository": "https://github.com/brickredsys/vision-ai-portal",
  "logo": "https://brickredsys.com/img/logo.png",
  "keywords": [
    "AI",
    "vision",
    "portal",
    "enterprise",
    "application development",
    "UI/UX design",
    "DevOps",
    "application modernization"
  ],
  "env": {
    "NEXTAUTH_URL": {
      "description": "Next.js authentication URL."
    },
    "NEXTAUTH_SECRET": {
      "description": "Next.js authentication secret."
    },
    "SMTP_HOST": {
      "description": "SMTP server host name."
    },
    "SMTP_PORT": {
      "description": "SMTP server port number."
    },
    "SMTP_USER": {
      "description": "SMTP server username."
    },
    "SMTP_PASSWORD": {
      "description": "SMTP server password."
    },
    "SMTP_FROM": {
      "description": "SMTP server sender's email address."
    },
    "DATABASE_URL": {
      "description": "Hosted Database URL."
    },
    "APP_URL": {
      "description": "Public root URL of the Vision AI Portal installation, replace <HEROKU_APP_NAME> with the app name.",
      "value": "https://<HEROKU_APP_NAME>.herokuapp.com"
    },
    "SVIX_URL": {
      "description": "SVIX URL.",
      "required": false
    },
    "SVIX_API_KEY": {
      "description": "SVIX API key.",
      "required": false
    }
  },
  "success_url": "/"
}
